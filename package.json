{
  "name": "jsdoc-md",
  "version": "3.0.0",
  "description": "A CLI to analyze source JSDoc and generate documentation under a given heading in a markdown file (such as readme.md).",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/jsdoc-md",
  "homepage": "https://github.com/jaydenseric/jsdoc-md#readme",
  "bugs": "https://github.com/jaydenseric/jsdoc-md/issues",
  "keywords": [
    "jsdoc",
    "md",
    "markdown",
    "readme",
    "babel",
    "remark",
    "unified"
  ],
  "files": [
    "lib",
    "index.js",
    "cli.js",
    "!*.test.js"
  ],
  "bin": "cli.js",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "doctrine": "^3.0.0",
    "github-slugger": "^1.2.1",
    "globby": "^9.2.0",
    "mdast-util-inject": "^1.1.0",
    "remark-behead": "^2.2.1",
    "remark-parse": "^6.0.3",
    "remark-stringify": "^6.0.4",
    "remark-toc": "^5.1.1",
    "unified": "^7.1.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "circular-json": "^0.5.9",
    "eslint": "^6.0.0",
    "eslint-config-env": "^6.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-import-order-alphabetical": "^0.0.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "tap": "^14.2.5"
  },
  "scripts": {
    "prepare": "npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:jsdoc": "node cli",
    "prepare:prettier": "prettier readme.md --write",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:js",
    "test:eslint": "eslint .",
    "test:prettier": "prettier '**/*.{json,yml,md}' -l",
    "test:js": "tap",
    "prepublishOnly": "npm test"
  }
}
