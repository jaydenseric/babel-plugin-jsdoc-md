{
  "name": "jsdoc-md",
  "version": "8.0.0",
  "description": "A CLI to analyze source JSDoc and generate documentation under a given heading in a markdown file (such as readme.md).",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/jsdoc-md",
  "homepage": "https://github.com/jaydenseric/jsdoc-md#readme",
  "bugs": "https://github.com/jaydenseric/jsdoc-md/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "jsdoc",
    "md",
    "markdown",
    "readme",
    "babel",
    "remark",
    "unified",
    "esm",
    "mjs"
  ],
  "files": [
    "cli",
    "private",
    "public"
  ],
  "main": "public",
  "exports": {
    ".": {
      "import": "./public/index.mjs",
      "require": "./public/index.js"
    },
    "./public/": "./public/",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "bin": {
    "jsdoc-md": "cli/jsdoc-md.js"
  },
  "engines": {
    "node": "^10.17.0 || ^12.0.0 || >= 13.7.0"
  },
  "dependencies": {
    "@babel/code-frame": "^7.10.4",
    "@babel/core": "^7.12.10",
    "arg": "^5.0.0",
    "comment-parser": "^0.7.6",
    "doctrine": "^3.0.0",
    "github-slugger": "^1.3.0",
    "globby": "^11.0.1",
    "kleur": "^4.1.3",
    "mdast-util-inject": "^1.1.0",
    "remark-behead": "^2.3.3",
    "remark-gfm": "^1.0.0",
    "remark-parse": "^9.0.0",
    "remark-stringify": "^9.0.1",
    "remark-toc": "^7.0.0",
    "unified": "^9.2.0"
  },
  "devDependencies": {
    "coverage-node": "^4.0.0",
    "disposable-directory": "^3.0.0",
    "eslint": "^7.15.0",
    "eslint-config-env": "^15.0.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "flatted": "^3.1.0",
    "hard-rejection": "^2.1.0",
    "prettier": "^2.2.1",
    "snapshot-assertion": "^2.0.0",
    "test-director": "^5.0.0"
  },
  "scripts": {
    "prepare": "npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:jsdoc": "node cli/jsdoc-md",
    "prepare:prettier": "prettier --write readme.md",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:api",
    "test:eslint": "eslint .",
    "test:prettier": "prettier -c .",
    "test:api": "coverage-node -r hard-rejection/register test",
    "prepublishOnly": "npm test"
  }
}
